{
  "name": "mirador-share-plugin",
  "version": "1.0.0-alpha.1",
  "description": "mirador-share-plugin React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:ProjectMirador/mirador-share-plugin.git"
  },
  "scripts": {
    "build": "npm run build:umd",
    "build:demo": "NODE_ENV=development webpack --mode=development",
    "build:umd": "NODE_ENV=production webpack --mode=production",
    "build:es": "mkdir -p es && cp -r src/* es && NODE_ENV=production MODULE_FORMAT=es npx babel es -d es",
    "build:cjs": "mkdir -p lib && cp -r src/* lib && NODE_ENV=production MODULE_FORMAT=cjs npx babel lib -d lib",
    "clean": "rm -rf ./umd && rm -rf ./es && rm -rf ./lib && rm -rf ./demo/dist",
    "lint": "eslint ./src ./__tests__",
    "prepublishOnly": "npm run clean && npm run build:es && npm run build:cjs && npm run build",
    "start": "NODE_ENV=development webpack serve --open",
    "test": "npm run lint && jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "notistack": "^3.0.1"
  },
  "peerDependencies": {
    "@mui/material": "^5.x",
    "mirador": "^4.0.0-alpha.5",
    "react": "18.x",
    "react-dom": "18.x"
  },
  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@mui/material": "^5.13.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.0",
    "@testing-library/user-event": "^14.4.3",    
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^24.8.0",
    "mirador": "^4.0.0-alpha.5",
    "react": "^18.0.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.0.0",
    "react-refresh": "^0.14.0",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.7.4"
  },
  "author": "",
  "homepage": "",
  "license": "Apache-2.0",
  "keywords": [
    "react-component"
  ]
}
